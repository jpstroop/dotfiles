[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    editor = code --wait
    excludesfile = ~/.gitignore
[commit]
    gpgsign = true
[gpg]
    program = gpg2
[pull]
    ff = only
[init]
    defaultBranch = main
